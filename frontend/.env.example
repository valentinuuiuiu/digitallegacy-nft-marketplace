# Frontend Environment Variables Template
# Copy this file to .env and fill in your actual values

# IPFS/Pinata Configuration (for file storage)
VITE_PINATA_API_KEY=your_pinata_api_key_here
VITE_PINATA_SECRET_KEY=your_pinata_secret_key_here

# Infura Configuration (for Ethereum RPC)
VITE_INFURA_PROJECT_ID=your_infura_project_id_here

# Application Configuration
VITE_APP_NAME=DigitalLegacy
VITE_APP_DESCRIPTION=NFT Marketplace for Digital Creators

# Network Configuration
VITE_DEFAULT_NETWORK=sepolia
VITE_ENABLE_DEMO_MODE=true
